class stack:
    def __init__(self):
        self.stack=[]
    def add(self,dataval):
      if dataval not in self.stack:
        self.stack.append(dataval)
        return True
      else:
        return False
    def peek(self):
      return self.stack[-1] 
Astack=stack ()
Astack.add("Mon")
Astack.add("tue")
Astack.peek()
print(Astack.peek()) 
Astack.add("wed")
Astack.add("thu")         
